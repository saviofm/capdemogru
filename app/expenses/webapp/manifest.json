{
    "_version": "1.32.0",
    "sap.app": {
        "id": "capdemowm.app.expenses",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "dataSources": {
            "mainService": {
                "uri": "/v2/catalog/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "OVP.smartovptemplate",
            "version": "1.41.1"
        },
        "crossNavigation": {
            "inbounds": {
                "capdemowm-app-expenses-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "overview",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.99.0",
            "libs": {
                "sap.ovp": {}
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties",
                "preload": false
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "mainService": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultCountMode": "Inline"
                }
            }
        },
        "extends": {
            "extensions": {}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    },
    "sap.ovp": {
        "globalFilterModel": "mainService",
        "globalFilterEntityType": "Expenses",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
        "cards": {
            "list01": {
              "model": "mainService",
              "template": "sap.ovp.cards.list",
              "settings": {
                  "title": "Expense List",
                  "entitySet": "Expenses",
                  "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                  "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                  "annotationPath": "com.sap.vocabularies.UI.v1.LineItem"
              }
            },
            "chart01": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Expense Chart",
                    "entitySet": "ExpensesAnalytics",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart"
                }
            },
            "chart02": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Expense Chart",
                    "entitySet": "Expenses",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart"
                }
            },
            "chart03": {
                "model": "mainService",
                "template": "sap.ovp.cards.table",
                "settings": {
                    "entitySet": "Expenses",
                    "category": "Categoria",
                    "title": "Tabela"
                }
            }
          }
    }
}